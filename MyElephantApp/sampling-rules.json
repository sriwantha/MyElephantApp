{
  "version": 1,
  "rules": [
    {
      "description": "ElephantWebAPI",
      "service_name": "*",
      "http_method": "*",
      "url_path": "/api/*",
      "fixed_target": 0,
      "rate": 1
    },
    {
      "description": "ElephantContacts",
      "service_name": "*",
      "http_method": "*",
      "url_path": "/Contacts/*",
      "fixed_target": 0,
      "rate": 1
    }
  ],
  "default": {
    "fixed_target": 1,
    "rate": 0.1
  }
}